{
  "name": "mark-domkan/env-token-auth",
  "description": "Adds the ability to use environtment token for the auth:api middleware.",
  "homepage": "https://github.com/markdomkan/env-token-auth",
  "license": "MIT",
  "authors": [
    {
      "name": "Mark Domkan"
    }
  ],
  "require": {
    "php": ">=7.2.0"
  },
  "autoload": {
    "psr-4": {
      "MarkDomkan\\EnvTokenAuth\\": "./src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "MarkDomkan\\EnvTokenAuth\\EnvTokenAuthServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
